{
  "name": "boxagenciawebchat",
  "description": "boxagenciawebchat",
  "productName": "ChatBoxAgenciaWeb",
  "main": "app/main.js",
  "version": "1.0.44",
  "author": {
    "name": "Paloma Macetko",
    "email": "cmacetko@gmail.com",
    "url": "https://github.com/cmacetko"
  },
  "license": "MIT",
  "homepage": "",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@electron-forge/publisher-github": "^6.2.1",
    "@electron-forge/publisher-s3": "^6.2.1",
    "babel-jest": "^25.3.0",
    "electron": "7.2.4",
    "electron-react-devtools": "^0.5.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-livereload": "^4.0.1",
    "jest": "^25.3.0",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "dochelper": "^1.0.4",
    "electron-app-settings": "^1.2.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-google-analytics": "^1.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.4.8",
    "electron-notify": "^0.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^6.1.1",
    "electron-windows-badge": "^1.1.0",
    "fs": "^0.0.1-security",
    "ini": "^1.3.5",
    "update-electron-app": "^2.0.1"
  },
  "build": {
    "mac": {
      "icon": "assets/icon.icns"
    },
    "win": {
      "icon": "assets/icon.ico"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "boxagenciawebchat"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "cmacetko",
              "name": "boxagenciawebchat",
              "draft": true
            }
          }
        }
      ]
    }
  }
}
